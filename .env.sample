# Sample environment configuration for testing
# Copy this to .env and adjust values as needed

# Number of days to fill with content
daysToFillCount=1

# Batch size (number of images per day)
batchSize=2

# Source folder with images to distribute
sourceFolder=./sample/test_source

# Target root folder where organized images will be placed
targetRootFolder=./sample/test_target

# Telegram API credentials
TELEGRAM_API_ID=
TELEGRAM_API_HASH=
TELEGRAM_PHONE=
TELEGRAM_CHAT_ID=